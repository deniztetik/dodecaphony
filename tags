ensure_number_scale	lib/pitch.rb	/^  def ensure_number_scale number$/;"	f	class:Pitch
generate_pitch_number	lib/pitch.rb	/^  def generate_pitch_number pitch_string$/;"	f	class:Pitch
initialize	lib/dodecaphony.rb	/^  def initialize tone_row$/;"	f	class:Dodecaphony
initialize	lib/pitch.rb	/^  def initialize pitch_name$/;"	f	class:Pitch
normalize_row	lib/dodecaphony.rb	/^  def normalize_row message$/;"	f	class:Dodecaphony
p0	lib/dodecaphony.rb	/^  def p0$/;"	f	class:Dodecaphony
spell_as_flat	lib/pitch.rb	/^  def spell_as_flat$/;"	f	class:Pitch
